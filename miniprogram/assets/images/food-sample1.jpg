<svg xmlns="http://www.w3.org/2000/svg" width="200" height="150" viewBox="0 0 200 150">
  <!-- 苹果图片的SVG表示 -->
  <rect width="200" height="150" fill="#f8f9fa"/>
  
  <!-- 苹果主体 -->
  <ellipse cx="100" cy="90" rx="35" ry="40" fill="#ff6b6b"/>
  
  <!-- 苹果高光 -->
  <ellipse cx="85" cy="75" rx="12" ry="15" fill="#ff8e8e" opacity="0.7"/>
  
  <!-- 苹果叶子 -->
  <ellipse cx="110" cy="55" rx="8" ry="12" fill="#51cf66" transform="rotate(30 110 55)"/>
  
  <!-- 苹果柄 -->
  <rect x="98" y="50" width="4" height="8" fill="#8b4513"/>
  
  <!-- 文字标签 -->
  <text x="100" y="130" text-anchor="middle" font-family="Arial, sans-serif" font-size="14" fill="#333">新鲜苹果</text>
</svg>